# Dotfiles scripts
export PATH="$HOME/.dotfiles/bin:$PATH"

# repository bin - mkdir .git/safe in the root of repositories you trust
export PATH=".git/safe/../../bin:$PATH"

# sencha
export SENCHA_CMD_3_0_0="$HOME/Development/Sencha/bin/Sencha/Cmd/4.0.2.67"
export PATH=$SENCHA_CMD_3_0_0:$PATH

# homebrew
export PATH="/usr/local/bin:$PATH"

# npm bin
export PATH="/usr/local/share/npm/bin:$PATH"

# postgres
export PATH="/Applications/Postgres.app/Contents/MacOS/bin:$PATH"

# android bin
export ANDROID_BIN="$HOME/Development/adt-bundle/sdk/tools/android"
export PATH="$HOME/Development/adt-bundle/sdk/tools:$PATH"
export PATH="$HOME/Development/adt-bundle/sdk/platform-tools:$PATH"

